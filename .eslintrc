{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "react/prop-types": [0],
    "react/display-name":[0],
    "no-console":0
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
